<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/hd_fill.wrap_land"
    android:background="@drawable/es_main_bg">

    <include layout="@layout/hd_navbar" />

    <LinearLayout
        style="@style/hd_fill"
        android:baselineAligned="false"
        android:padding="15.0dip">

        <include layout="@layout/hd_plugin_tradeinfo" />

        <hdtx.androidsdk.ui.APScrollView
            android:id="@+id/hd_id_ScrollView"
            style="@style/hd_fill"
            android:fillViewport="true">

            <LinearLayout
                style="@style/hd_cont_land"
                android:paddingLeft="10.0dip"
                android:paddingRight="10.0dip">

                <LinearLayout
                    android:id="@+id/hd_id_PayTypeLayout"
                    android:layout_width="fill_parent"
                    android:layout_height="40.0dip"
                    android:gravity="center_vertical">

                    <TextView
                        style="@style/hd_text"
                        android:layout_marginLeft="8.0dip"
                        android:gravity="center_vertical"
                        android:text="@string/hd_pay_channel"
                        android:textColor="@color/hd_em4_color" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/hd_id_wxChannelid"
                    style="@style/hd_dash" />

                <LinearLayout
                    android:id="@+id/hd_id_RecoChannleLayout"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/hd_drawable_itembtn"
                    android:gravity="center_vertical">

                    <GridView
                        android:id="@+id/hd_id_ChannelReco"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:cacheColorHint="@android:color/transparent"
                        android:fadingEdge="none"
                        android:listSelector="@android:color/transparent" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/hd_id_wxChannelLayout"
                    style="@style/hd_itemchannel">

                    <ImageView
                        android:id="@+id/hd_id_wxChannelIcon"
                        style="@style/hd_wrap.wxchannel" />

                    <LinearLayout
                        style="@style/hd_linear"
                        android:layout_weight="100.0"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/hd_id_wxChannelText"
                            style="@style/hd_text"
                            android:text="@string/hd_wx_pay" />
                    </LinearLayout>

                    <ImageView style="@style/hd_iconarrow" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/hd_id_list_title"
                    style="@style/hd_dash" />

                <LinearLayout
                    android:id="@+id/hd_id_aliChannelLayout"
                    style="@style/hd_itemchannel">

                    <ImageView
                        android:id="@+id/hd_id_aliChannelIcon"
                        style="@style/hd_wrap.alichannel" />

                    <LinearLayout
                        style="@style/hd_linear"
                        android:layout_weight="100.0"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/hd_id_aliChannelText"
                            style="@style/hd_text"
                            android:text="@string/hd_ali_pay" />
                    </LinearLayout>

                    <ImageView style="@style/hd_iconarrow" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/hd_id_aliChannelid"
                    style="@style/hd_dash" />

                <LinearLayout
                    android:visibility="gone"
                    android:id="@+id/hd_id_jfChannelLayout"
                    style="@style/hd_itemchannel">

                    <ImageView
                        android:id="@+id/hd_id_jfChannelIcon"
                        style="@style/hd_wrap.ydchannel" />

                    <LinearLayout
                        style="@style/hd_linear"
                        android:layout_weight="100.0"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/hd_id_jfChannelText"
                            style="@style/hd_text"
                            android:text="@string/hd_jf_pay" />
                    </LinearLayout>

                    <ImageView style="@style/hd_iconarrow" />
                </LinearLayout>

                <ImageView
                    android:visibility="gone"
                    android:id="@+id/hd_id_jfChannelid"
                    style="@style/hd_dash" />

                <LinearLayout
                    android:visibility="gone"
                    android:id="@+id/hd_id_ylChannelLayout"
                    style="@style/hd_itemchannel">

                    <ImageView
                        android:id="@+id/hd_id_ylChannelIcon"
                        style="@style/hd_wrap.ylchannel" />

                    <LinearLayout
                        style="@style/hd_linear"
                        android:layout_weight="100.0"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/hd_id_ylChannelText"
                            style="@style/hd_text"
                            android:text="@string/hd_yl_pay" />
                    </LinearLayout>

                    <ImageView style="@style/hd_iconarrow" />
                </LinearLayout>

                <ImageView
                    android:visibility="gone"
                    android:id="@+id/hd_id_ylChannelid"
                    style="@style/hd_dash" />


                <LinearLayout
                    android:id="@+id/hd_id_moreChannelLayout"
                    style="@style/hd_itemchannel">


                    <LinearLayout
                        style="@style/hd_linear"
                        android:layout_weight="100.0"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_marginLeft="10dip"
                            android:layout_marginTop="5dip"
                            android:layout_marginBottom="5dip"
                            style="@style/hd_text"
                            android:text="@string/hd_more_pay" />

                    </LinearLayout>

                    <TextView
                        android:id="@+id/hd_id_moreChannelText"
                        android:layout_marginRight="10dip"
                        android:layout_marginTop="5dip"
                        android:layout_marginBottom="5dip"
                        style="@style/hd_text"
                        android:text="@string/hd_more_pay_mark" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/hd_id_moreChannelid"
                    style="@style/hd_dash" />


                <LinearLayout
                    android:id="@+id/hd_id_webChannelLayout"
                    style="@style/hd_itemchannel"
                    android:visibility="gone">

                    <ImageView
                        android:id="@+id/hd_id_webChannelIcon"
                        style="@style/hd_wrap.webchannel" />

                    <LinearLayout
                        style="@style/hd_linear"
                        android:layout_weight="100.0"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/hd_id_webChannelText"
                            style="@style/hd_text"
                            android:text="@string/hd_web_pay" />
                    </LinearLayout>

                    <ImageView style="@style/hd_iconarrow" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/hd_id_webChannelid"
                    style="@style/hd_clear" />
            </LinearLayout>
        </hdtx.androidsdk.ui.APScrollView>
    </LinearLayout>

</LinearLayout>